<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="login">
	    
	   <resultMap type="com.hhhmemories.cloud.member.service.MemberVO" id="selectMemberInfo">
          <result column="" property=""/>
       </resultMap>
       
       
	    <select id="selectMemberInfo" 
	    	resultType="com.hhhmemories.cloud.member.service.MemberVO">
	    	/* MEMBER LOGIN > SELECT ONE START */
	    	SELECT 
	    		member_id 
	    	FROM 
	    		tb_member 
	    	WHERE 
	    		member_id = #{memberId };	
	    	/* MEMBER LOGIN > SELECT ONE END */
	    </select>
</mapper>

